    <section id="page-content-wrap">
      <div class="register-page-wrapper section-padding">
        <div class="container">
          <div class="row">
            <div class="col-lg-12">
              <div class="register-page-inner">
                <div class="col-lg-10 m-auto">
                  <div class="register-form-content">
                    <div class="row">
                      <!-- Regsiter Form Area Start -->
                      <div class="col-lg-12 col-md-12">
                        <div class="register-form-wrap">
                          <%  try{
                            if(!(request.getParameter("result").equals(null))){%>
                        <div class="col-lg-8"> <p class="alert alert-success"><a href="#" class="close" data-dismiss="alert">&times;</a><%=request.getParameter("result")%></p></div>
                        <%}}catch(Exception e){}%>
                          <h3>registration Form</h3>
                          <div class="register-form">
                            <form action="../../controller/alumni/register.jsp">
                              <div class="row">
                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <label for="register_email">Email</label>
                                    <input
                                      type="email"
                                      size="25"
                                      class="form-control"
                                      id="register_email"
                                      name="email"
                                      required pattern="[^ @]+@[^ @]+\.[a-zA-z]{2,6}"
                                      oninvalid="this.setCustomValidity('Email is not in the proper format.')"
                                      oninput="this.setCustomValidity('')"
                                    />
                                  </div> 
                                </div>

                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <label for="register_password"
                                      >Password</label
                                    >
                                    <input
                                      type="password"
                                      class="form-control"
                                      id="register_password"
                                      name="password"
                                      required pattern=".{6,}" 
                                      oninvalid="this.setCustomValidity('Password must be of 6 characters.')" 
                                      oninput="this.setCustomValidity('')"
                                    />
                                  </div>
                                </div>
                              </div>
                              <div class="row">
                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <label for="register_name">Name</label>
                                    <input
                                      type="text"
                                      class="form-control"
                                      id="register_name"
                                      name="name"
                                      required oninvalid="this.setCustomValidity('Name is required.')"
                                      oninput="this.setCustomValidity('')"
                                    />
                                  </div>
                                </div>

                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <label for="register_stuid"
                                      >Enrollment Number</label
                                    >
                                    <input
                                      type="text"
                                      class="form-control"
                                      id="enrollement_number"
                                      name="prn"
                                      required oninvalid="this.setCustomValidity('PRN/Roll number is required.')"
                                      oninput="this.setCustomValidity('')"
                                    />
                                  </div>
                                </div>
                              </div>
                              <div class="row">
                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <label for="programme">College</label>
                                    <!-- The list needs to be changed to the list of colleges under DHE -->
                                    
                                        <select name="programme" 
                                        required oninvalid="this.setCustomValidity('College is required.')"
                                        oninput="this.setCustomValidity('')"
                                        class="form-control"
                                        id="college">
                                       <option>CM</option> 
                                      <option>IF</option>
                                      <option>ME</option>
                                       <option>EE</option>
                                       <option>CE</option>
                                      <option>PE</option>
                                       <option>E&TC</option>
                                      <option>DDGM</option>
                                      <option>IDD</option>
                                       <option>AE</option>
                                     </select>
                                  </div>
                                </div>
                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <label for="register_year"
                                      >Department</label
                                    >
                                    <!-- Options need to be changed to a dynamic drop down list -->
                                    <select name="department" 
                                    required oninvalid="this.setCustomValidity('Term is required.')"
                                    oninput="this.setCustomValidity('')"
                                    type="text"
                                    class="form-control"
                                    id="dept"
                                    name="dept">
                                    <option>Chemistry</option>
                                    <option>Management Studies</option> 
                                    </select> 
                                  </div>
                                </div>
                              </div>
                              <div class="row">
                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <label for="register_deptno"
                                      >Admission Year</label
                                    >
                                    <select name="admissionYear"
                                      class="form-control"
                                      id="admn_year" 
                                      required oninvalid="this.setCustomValidity('Admission Year is required.')"
                                      oninput="this.setCustomValidity('')">
                                      <option>2017</option>
                                      <option>2018</option>
                                      <option>2019</option> 
                                   </select>
                                  </div>
                                </div>
                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <label for="register_year"
                                      >Passing Year</label
                                    >
                                    <select name="passOutYear" 
                                    required oninvalid="this.setCustomValidity('Pass Out Year is required.')"
                                    oninput="this.setCustomValidity('')"
                                    class="form-control"
                                    id="pass_year">
                                      <option>2017</option>
                                      <option>2018</option>
                                      <option>2019</option> 
                                   </select>
                                  </div>
                                </div>
                              </div>
                              <div class="row">
                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <label for="register_name"
                                      >Date of Birth</label
                                    >
                                    <input
                                      type="date"
                                      class="form-control"
                                        id="dob"
                                      name="dob" required
                                    />
                                  </div>
                                </div>

                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <label for="register_stuid"
                                      >Phone Number</label
                                    >
                                    <input
                                      type="tel"
                                      class="form-control"
                                      id="phno"
                                      name="mobileNo"
                                      required pattern=".{10}" 
                                      oninvalid="this.setCustomValidity('Mobile number must be of 10 characters.')" 
                                      oninput="this.setCustomValidity('')"
                                    />
                                  </div>
                                </div>
                              </div>
                              <div class="row">
                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                      <!-- Hatim has made it "country" and "state" instead of address. Leaving this 
                                      unedited for now -->
                                    <label for="register_name">Country</label>
                                    <input
                                      type="text"
                                      class="form-control"
                                      id="countr"
                                      name="country"
                                    />
                                  </div>
                   
                                </div>

                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <label for="register_stuid">State</label>
                                    <input
                                      type="text"
                                      class="form-control"
                                      id="state"
                                      name="state"
                                    />
                                  </div>
                                </div>
                              </div>

                              <div class="gender form-group">
                                <label class="g-name d-block">Gender</label>
                                <div
                                  class="custom-control custom-radio custom-control-inline"
                                >
                                  <input
                                    type="radio"
                                    id="register_gender_male"
                                    name="gender"
                                    value="mail"
                                    required oninvalid="this.setCustomValidity('Gender is required.')"
                                    oninput="this.setCustomValidity('')"
                                    class="custom-control-input"
                                  />
                                  <label
                                    class="custom-control-label"
                                    for="register_gender_male"
                                    >Male</label
                                  >
                                </div>
                                <div
                                  class="custom-control custom-radio custom-control-inline"
                                >
                                  <input
                                    type="radio"
                                    id="register_gender_female"
                                    name="gender"
                                    value="female"
                                    class="custom-control-input"
                                  />
                                  <label
                                    class="custom-control-label"
                                    for="register_gender_female"
                                    >Female</label
                                  >
                                </div>
                              </div>
                              <div class="row">
                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <div
                                      class="custom-control custom-checkbox float-lg-left"
                                    >
                                      <input
                                        type="checkbox"
                                        class="custom-control-input"
                                        id="customCheck1"
                                      />
                                      <label
                                        class="custom-control-label"
                                        for="customCheck1"
                                      >
                                        I have read and agree to the
                                        <a href="#">Alumni</a> Terms of
                                        Service</label
                                      >
                                    </div>
                                  </div>
                                </div>
                              </div>
                              <div class="row">
                                <div class="col-12 col-sm-6">
                                  <div class="form-group">
                                    <input
                                      type="submit"
                                      class="btn btn-reg"
                                      value="Registration"
                                    />
                                  </div>
                                </div>
                              </div>
                            </form>
                          </div>
                        </div>
                      </div>
                      <!-- Register Form Area End -->
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
